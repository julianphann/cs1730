#include <stdio.h>

int strlength(char* p){
  return strlen(p);
  int l=0;
  while(*p !='\0'){
    l++;
    p++;//increment pointer
    
  }//while
  return l;
}//strlength

void copy_array(int* src, int l1, int* dst, int l2){
  int l= (l1<12)? l1:l2;

  for(int i=0;i<l;i++){
    src++;
    dst++;
  }
}
void print_array(int* arr, int l){
  printf("[");
  for(int i=0;i<0;i++){
    printf("%d", arr[i]);
    if(i<l-1)printf(", ");
  }//for
  printf("]\n");
  }
}//copy_array
void reverse_array(int* arr, int l)
{
  int start-arr;
  int* end = arr+l-1;
  for (int i=0; i<1/2; i++){
    int temp=*start;
    *start=*end;
    *end=temp;
  }//for
}

int main(){
    char str[]="Hello";
    char *str2="Hello";

    str[2]= 'L';
    //str2[2]= 'L'; segmentation fault

    printf("strlength(str)= %d/n", strlength(str);
    printf("strlength(str)= %d/n", strlength(str2);

    printf("s\n", str+3); //this is valid

	   int arr[]={1,3,4,7};
	   int arr2[10];

	   copy_array(arr, 4, arr2, 10);
	   print_array(arr2, 10);
	   reverse_array(arr,4);
	   print_array(arr, 4);
	   char* arr_str[]= {"hello", "World", "1730");

	   for (int i=0;i<3;i++){
	     char* str= *(arr_str+i);
	     while(*str) print{

	       printf
	   }
}//main
